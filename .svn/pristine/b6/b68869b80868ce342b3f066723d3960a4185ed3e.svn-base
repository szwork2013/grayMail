<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml" id="pg-window"><head>    <title>保存到我的相册</title>    <meta content="text/html; charset=utf-8" http-equiv="Content-Type" />    <script type="text/javascript">    var v="20120531";    //设域    document.domain=window.location.host.match(/([^.]+\.[^.:]+):?\d*$/)[1];    CommonInit(document, ["js/m2011.disk.upload.js", "js/m2011.disk.savemailattachtoalbum.js" ]);function CommonInit(docCur, diskResJs, dCssaAble){    var doc = docCur || document;    //获得资源路径    var resourcePath = window.top.resourcePath;    var diskResourcePath = resourcePath.replace("coremail", "newnetdisk4") + "/";   //资源    var htmlCode = "";    if(dCssaAble == undefined || dCssaAble == true) {       htmlCode += getLinkTag(resourcePath + "/css/netdisk.css");    }    htmlCode += getLinkTag("");   //保留皮肤的css    top.loadScript("jquery.js", doc);    top.loadScript("m2011.utilscontrols.pack.js", doc);	htmlCode += getJsTag(diskResourcePath + "/js/m2011.disk.diskconf.js");    htmlCode += getJsTag(diskResourcePath + "/js/m2011.disk.diskcommon.js");        if(diskResJs && diskResJs.length > 0){        for(var i=0;i<diskResJs.length ;i++){           if((/\.js$/i).test(diskResJs[i])){htmlCode += getJsTag(diskResourcePath + diskResJs[i])};           if((/\.css$/i).test(diskResJs[i])){htmlCode += getLinkTag(diskResourcePath + diskResJs[i])};        }    }      doc.write(htmlCode);}function getJsTag(path,charset){    var html="<script charset='utf-8' src='"+ path +"?v="+ v +"' type='text/javascript'></"+"script>";    if (charset) html = html.replace("utf-8", charset);    return html;}function getLinkTag(path){    var text="";    if(path)text="href='"+ path +"?v="+ v +"'";    return "<link rel='stylesheet' type='text/css' "+ text +" />";}    </script>    <script type="text/javascript">    //加载皮肤    Utils.loadSkinCss(null,document, "netdisk");    $().ready(PageInit);    </script></head>    <body >        <!--保存到我的相册-->        <div class="dialog">	        <div class="bd">		        <table cellpadding="0" class="tbl-dialog-form dl-form-upload">			        <tr>				        <th>上传位置：</th>				        <td>                            <select id="selDir">                            </select>                            <a href="javascript:;" id="aAdd">新建相册</a>                            <span id="newPhoto" style="display:none;">                                <input type="text" id="dirName" class="text w120" maxlength="12" />                                <a href="javascript:;" id="aAddAlbum">添加</a>                                <a href="javascript:;" id="aCancel1">取消</a>                            </span>                            <p style="display:none;" class="error-p"></p>                        </td>			        </tr>		        </table>                	        </div>            <div class="ft">		        <ul class="dbtn-list">			        <li><a class="btnNormal" href="javascript:;" id="aSaveAttach">确 定</a></li>			        <li><a class="btnNormal" href="javascript:;" id="aCancel">取 消</a></li>		        </ul>	        </div>        </div>    </body></html>