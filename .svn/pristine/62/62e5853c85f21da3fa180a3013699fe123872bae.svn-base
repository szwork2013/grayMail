html,body{height:100%;}
a:hover{text-decoration:none;}
.topbar{height:44px; line-height:44px; padding:0 10px; background:#f5f5f5; border-bottom:1px solid #a5a5a5;
        background: linear-gradient(#ffffff 0%,#ececec 100%);
        background: -moz-linear-gradient(top,#ffffff,#ececec);
        background: -webkit-gradient(linear,0% 0%,0% 100%,from(#ffffff),to(#ececec));
        background: -o-linear-gradient(top,#ffffff,#ececec);
        filter:progid:DXImageTransform.Microsoft.Gradient(GradientType=1,EndColorStr='#ececec',StartColorStr='#ffffff');
}
.squareMain{width:762px; padding:10px 0 10px 10px;}

.icon{background:url(../../../images/module/schedule/square.png) no-repeat; _background:url(../../../images/module/schedule/square8.png) no-repeat;display:inline-block; overflow:hidden;}
.prev{background-position:-80px -24px; width:34px; height:50px;}
.next{background-position:-120px -24px; width:34px; height:50px;}
.i_life{background-position:-40px 0px; width:18px; height:16px;}
.i_match{background-position:-1px -1px; width:14px; height:18px; position:relative; top:2px;}
.i_movie{background-position:-17px 1px; width:18px; height:16px;}
.i_school{background-position:-64px 0px; width:22px; height:13px;}
.i_shopping{background-position:-90px -1px; width:16px; height:14px;}

.squareMain .nav{position:relative;}
.squareMain .nav .prev,.squareMain .nav .prev{position:absolute; top:85px;}
.squareMain .nav .next{position:absolute; top:85px; right:0;}
.squareMain .nav .prev:hover{background-position:0px -24px;}
.squareMain .nav .next:hover{background-position:-40px -24px;}
.squareMain .nav .readBTn{margin-top:30px; text-align:center;}
/*透明层*/
.layerDiv{position:absolute; right:70px; top:0;
          filter: progid:DXImageTransform.Microsoft.gradient(enabled='true',startColorstr='#7F000000', endColorstr='#7F000000');
          background-color: rgba(0,0,0,0.7);
         -webkit-transition: all .7s ease-out .1s;/*渐变*/
         -moz-transition: all .7s ease-out .1s;
         transition: all .7s ease-out .1s;
}
.movieLay{width:160px;height:40px; top:auto; left:0; bottom:0; padding:10px;}
