.textInfoP{padding:15px;}
.textInfoP h3{color:#fff; font-size:14px; text-overflow:ellipsis; overflow:hidden; white-space:nowrap;}
.textInfoP p.describe{color:#cccccc; margin-top:5px;}
/* ============== 五角星评分 ============== */
.starFive{position: relative;top:4px;height: 12px;overflow:hidden;font-size: 0;display: inline-block;}
.starFive .starbg{width: 80px;height: 12px;background-position: 0 0;overflow: hidden;vertical-align: baseline; }
.starFive .starNum{width:60px;position: absolute;left:0;top:0;height: 12px;background-position: 0 -15px;overflow: hidden;vertical-align: baseline;}
.starFiveB{vertical-align:middle;position: relative;height: 17px;overflow:hidden;font-size: 0;display: inline-block;cursor: pointer;}
.starbg,.starNum{background:url(../../../images/module/schedule//yelStar.png) repeat-x 0 0;_background:url(../../../images/module/schedule//yelStar8.png) repeat-x 0 0; display:inline-block;}

.starFiveB .starNum{position: absolute;left:0;top:0;height: 17px;background-position: 0 -379px;overflow: hidden;vertical-align: baseline;}
.startGrade{position: relative;top: -4px;left: 2px;color: #fff;}
/* ============== 五角星评分 ============== */
.scheduleRssListColumn{margin-top:14px;}
.moveRecommend li{position:relative; float:left;}
.textInfoDl dt{ color:#fff; text-overflow:ellipsis; overflow:hidden; white-space:nowrap; margin-bottom:5px;}
.moveRecommend li.mr_14{margin-right:14px;}
/*赛事公告*/
.matchShow{margin-top:14px;}
.matchBox{height:376px;}
.matchBox h2{-bottom:1px solid #eaeaea;font-size:0; font-weight:normal; color:#528e51;}
.matchBox h2 i{margin-right:5px; *position:relative; *top:-2px;}
.matchBox h2 span{display:block;  font-size:16px; padding:4px 0 4px 10px; _padding:4px 0 0px 10px; background:#f7f7f7;  border-bottom:1px solid #eaeaea;}
.matchBox ul{margin-top:13px; overflow:hidden;}
.matchBox ul li em{display:inline-block; width:162px; _width:152px; text-overflow:ellipsis; overflow:hidden; white-space:nowrap; float:left;}
.matchBox ul li{line-height:32px; list-style:none; color:#666666; padding:0 15px; overflow:hidden; clear:both;}
.matchBox ul li span{color:#b9b9b9; float:left; margin-right:3px;}
.life{width:242px; float:right; border:1px solid #cccccc;}
.everMach{width:243px; height:182px;border:1px solid #cccccc; margin-right:14px; _margin-right:8px; float:left;}
/*btn*/
.unsuBtn{border:1px solid #fff;width: 40px;text-align:center;height: 20px;line-height: 19px; display: inline-block; color:#666;_position:relative; _top:5px;}
.unsuBtn:hover{border:1px solid #dadada; background-color:#fff; color:#444}
.blackBtn{border:1px solid transparent; color:#ccc; padding:2px 6px 0 6px;}
.blackBtn:hover{border:1px solid #999999; background-color:#fff; color:#444;}
