                                if (top.SendMail == 0)//
                                {
                                    top.SendMail = 1;
                                    var da = new Date();
                                    var url = "http://smsrebuild1.mail.10086.cn/sms/smsmms/superman/smshelp.htm?provid=" + top.UserData.provCode;
                                    var name = getUserName(data["var"].sendUserNumber.substring(2));
