}
.searchFilterList ul{padding:5px 10px 0 10px;}
.searchFilterList .a_open{padding:0 0 0 20px;margin-bottom: 10px;display: inline-block;}
.searchFilterList li{height:28px;}
.searchFilterList li.sela a{position: relative;}
.searchFilterList li.sela a .i_u_close{position: absolute;right:4px;top:9px;}

.searchFilterList li a{_zoom:1;overflow:hidden;padding:3px 10px;display:block;color:#333333;-webkit-border-radius:2px;-moz-border-radius:2px;border-radius:2px;}
.searchFilterList li .scontent{display:inline-block;height:18px;overflow:hidden;cursor: pointer;width:100%;vertical-align: top;}
.searchFilterList li a .stext{float: left;padding-right: 50px;word-wrap:break-word; word-break:break-all;}
.searchFilterList li a .snum{float: left;margin-left: -50px;display: inline;}

.searchFilterList li a:hover{background-color:#e5ebf4;text-decoration:none;color:#000;}
.searchFilterList li a.on{background-color:#d7e2f0;text-decoration:none;color:#000;font-weight: 700;}
.searchFilterList li label{display:inline-block;width:158px;*width:150px;overflow:hidden;position:relative;top:2px;top:4px\0;*top:0;}
.switchOn,.switchOff{display:inline-block;width:8px;height:37px;background:url(../../../images/global/global_24.png) no-repeat;_background:url(../../../images/global/global.png) no-repeat;background-position:-131px -39px;position:absolute;top:0px;left:-9px;z-index:3;}
.switchOn .i_triangle_h,.switchOff .i_triangle_h{position:absolute;left:4px;top:15px;}
.searchListoff .searchFilter{display: none;}


.switchOn .i_triangle_h{-webkit-transform: rotate(180deg);-moz-transform: rotate(180deg);transform: rotate(180deg);filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=2);}
.searchListon .searchFilter{display: block;margin-left: }
.searchListon .attath-list .td5{width: 236px;padding: 0;height: 38px;}
.searchListon .searchFilterTitle{display: block;}
.searchListon .attath-list th .thsearch {display: block;}
.searchListon .attadiv{margin-right: 222px;}
.searchListon .switchOn .i_triangle_h{-webkit-transform: rotate(0deg);-moz-transform: rotate(0deg);transform: rotate(0deg);filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=4);}
