/* 附件图标 */
.i_file,.i_file_16{width:32px;height:32px;background:url(../../images/global/fileico.png) no-repeat;display:inline-block;overflow:hidden;vertical-align:middle;}
.i_file_16{width:16px;height:16px;}
.i_f_xls,.i_f_xlsx{background-position:-68px -34px;}
.i_f_pdf{background-position:-34px 0;}
.i_f_doc,.i_f_docx{background-position:-68px 0;}
.i_f_ppt,.i_f_pptx{background-position:-102px 0;}
.i_f_fla,.i_f_swf{background-position:-136px 0;}
.i_f_mp3,.i_f_wav{background-position:-170px 0;}
.i_f_iso{background-position:-204px 0;}
.i_f_avi,.i_f_rmvb,.i_f_wmv,.i_f_flv{background-position:-238px 0;}
.i_f_fon,.i_f_ttc,.i_f_ttf{background-position:-272px 0;}
.i_f_bt{background-position:-306px 0;}
.i_f_txt{background-position:-340px 0;}
.i_f_html,.i_f_htm{background-position:-374px 0;}
.i_f_dvd{background-position:-408px 0;}
.i_f_rar,.i_f_zip,.i_f_7z{background-position:-442px 0;}
.i_f_exe,.i_f_apk,.i_f_ipa{background-position:0 -34px;}
.i_f_jpg,.i_f_jpeg,.i_f_jpe,.i_f_gif,.i_f_giff,.i_f_png,.i_f_bmp,.i_f_tif,.i_f_tiff{background-position:-34px -34px;}
.i_f_139{background-position:0 0;}
