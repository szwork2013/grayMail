 .i_m_xls, .i_m_xlsx{background-position:-288px -68px;}
 .i_m_pdf{background-position:-18px -68px;}
 .i_m_doc, .i_m_docx{background-position:-36px -68px;}
 .i_m_ppt, .i_m_pptx{background-position:-54px -68px;}
 .i_m_fla, .i_m_swf{background-position:-72px -68px;}
 .i_m_mp3, .i_m_wav{background-position:-90px -68px;}
 .i_m_iso{background-position:-108px -68px;}
 .i_m_avi, .i_m_rmvb, .i_m_wmv,.i_m_mlv{background-position:-126px -68px;}
 .i_m_fon, .i_m_ttc, .i_m_ttf{background-position:-144px -68px;}
 .i_m_bt{background-position:-162px -68px;}
 .i_m_txt{background-position:-180px -68px;}
 .i_m_html, .i_m_htm{background-position:-198px -68px;}
 .i_m_dvd{background-position:-216px -68px;}
 .i_m_rar, .i_m_zip, .i_m_7z{background-position:-234px -68px;}
 .i_m_exe, .i_m_apk, .i_m_ipa{background-position:-252px -68px;}
 .i_m_jpg, .i_m_jpeg, .i_m_ico, .i_m_jfif, .i_m_jpe, .i_m_gif, .i_m_giff, .i_m_png, .i_m_bmp, .i_m_tif, .i_m_tiff{background-position:-270px -68px;}
 .i_m_139{background-position:0 -68px;	}
