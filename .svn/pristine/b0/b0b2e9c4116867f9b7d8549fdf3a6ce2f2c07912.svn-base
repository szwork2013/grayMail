/* 邮件分类 */
.sorttable{border:1px solid #dfdfe1;width:100%;margin-top:20px;table-layout: fixed;}
.sorttable th{height:32px;border-bottom:1px solid #dfdfe1;background:url(../../images/module/set/tabtabx.png) repeat-x;}
.sorttable td{border-bottom:1px solid #dfdfe1;}
.sorttable td{padding:10px;text-align: left;}
.sorttable td.hover{background-color: #deeed7;border-bottom: 2px solid #000000;}
.sorttable th{padding:0 10px;text-align: left;}
.sorttable .td3{width: 90px;padding-left: 20px;}
.sorttable .td2{width: 110px;padding-left: 20px;}
.sorttable .td1 .brline{word-wrap:break-word; word-break:break-all;}

.sorttable .sort-r,.sorttable .sort-w{padding-left: 16px;}
.sort-r{background-position:-306px -496px; }
.sort-w{background-position:-307px -517px; }
.dragdiv{position: relative;padding-left: 15px;_zoom:1;}
.dragdiv .dragBar{position:absolute;left: 0;top:50%;margin-top: -5px;}

/* 短信通知修改 */
.setForm .bgfa{background-color:#fafafa; border:#f0f0f0 solid 1px; position: relative;}
.setForm .bgfa .setnotifytb{ width: 92%;}
.setForm .bgfa .noticfyadd{width:544px; background-color: #fff; margin-top: -6px; padding: 0 10px 0;zoom:1; position: relative;}
.setForm .bgfa .notifylisttab{margin-top: 0; margin-bottom: 10px;}
.setForm .formLineNone{border-top: none; padding: 10px 0 6px;}


/*表格*/
.setTableNew{border-collapse:collapse; table-layout:fixed; width:570px; border:1px solid #dfdfe1;}
.setTableNew td, .setTableNew th{border-bottom:1px solid #dfdfe1; height:30px;}
.setTableNew th{text-align:left; padding-left:14px; font-weight:normal;}
.setTableNew th .checkbox{margin-right:8px;}
.setTableNew td{text-align:right; padding-right:10px;}

/*副标题*/
.setArea h3{border-bottom:1px solid #dfdfe2;padding:0 0 3px 10px; font-weight:normal;}

.subList .big li.bottomLi{height:auto; overflow:visible;}
.setWrap .setArea h2{border-bottom:1px solid #dfdfe2;}
.setArea .cloudMailMainListBox{border-bottom:1px solid #ededed;}
.setArea .coundRightFrame{margin-left:0;}

.subList .big li a{height:28px; line-height:28px;}
.subList a .t_blackDown{top:13px;}
.subList a .t_blackRight{top:10px;}


