.btnGreen, .btnGray{height:34px; line-height:34px; display:inline-block; font-size:14px;
	-moz-box-shadow:0 0 1px #cecece;
    -webkit-box-shadow:0 0 1px #cecece;
    box-shadow:0 0 1px #cecece;

	-moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius:3px;
}
.btnGreen{border:1px solid #00a012; padding:0 20px; color:#fff;
	background:#00bc16;
	background: -webkit-gradient(linear, left 70%, left bottom, from(#00c116), to(#00b615));
	background: -moz-linear-gradient(top,  #00c116,  #00b615);
	background: -o-linear-gradient(left, #00c116, #00b615);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#00c116', endColorstr='#00b615');
	background: linear-gradient(left, #00c116, #00b615);
}
.btnGreen:hover{color:#fff;
	background:#00b315;
	background: -webkit-gradient(linear, left 70%, left bottom, from(#00c016), to(#00a313));
	background: -moz-linear-gradient(top,  #00c116,  #00a313);
	background: -o-linear-gradient(left, #00c116, #00a313);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#00c116', endColorstr='#00a313');
	background: linear-gradient(left, #00c116, #00a313);
}
.btnGreen:active{
	background:#00b315;
	background: -webkit-gradient(linear, left 70%, left bottom, from(#00be16), to(#009011));
	background: -moz-linear-gradient(top,  #00be16,  #009011);
	background: -o-linear-gradient(left, #00be16, #009011);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#00be16', endColorstr='#009011');
	background: linear-gradient(left, #00be16, #009011);
}
.btnGray{border:1px solid #cecece; padding:0 14px; color:#666;
	background:#fafafa;
	background: -webkit-gradient(linear, left 70%, left bottom, from(#ffffff), to(#f4f4f4));
	background: -moz-linear-gradient(top,  #ffffff,  #f4f4f4);
	background: -o-linear-gradient(left, #ffffff, #f4f4f4);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffff', endColorstr='#f4f4f4');
	background: linear-gradient(left, #ffffff, #f4f4f4);
}
.btnGray:hover{color:#666;
	background:#f3f3f3;
	background: -webkit-gradient(linear, left 70%, left bottom, from(#ffffff), to(#e9e9e9));
	background: -moz-linear-gradient(top,  #ffffff,  #e9e9e9);
	background: -o-linear-gradient(left, #ffffff, #e9e9e9);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffff', endColorstr='#e9e9e9');
	background: linear-gradient(left, #ffffff, #e9e9e9);
}
.btnGray:active{
	background:#f3f3f3;
	background: -webkit-gradient(linear, left 70%, left bottom, from(#eeeeee), to(#cfcfcf));
	background: -moz-linear-gradient(top,  #eeeeee,  #cfcfcf);
	background: -o-linear-gradient(left, #eeeeee, #cfcfcf);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#eeeeee', endColorstr='#cfcfcf');
	background: linear-gradient(left, #eeeeee, #cfcfcf);
}
.btnBlue, .btnBlue:hover, .btnDisabled{height:40px; line-height:40px; width:90px; text-align:center; font-size:14px; display:inline-block;
	-moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius:3px;
}
.btnBlue, .btnBlue:hover{border:1px solid #c7d3ec; color:#1a75ca;
	background:#e2effe;
	background: -webkit-gradient(linear, left 70%, left bottom, from(#f2f6ff), to(#d1e7fc));
	background: -moz-linear-gradient(top,  #f2f6ff,  #d1e7fc);
	background: -o-linear-gradient(left, #f2f6ff, #d1e7fc);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#f2f6ff', endColorstr='#d1e7fc');
	background: linear-gradient(left, #f2f6ff, #d1e7fc);
}
.btnDisabled, .btnDisabled:hover{border:1px solid #cecece; color:#666;
	background:#f3f3f3;
	background: -webkit-gradient(linear, left 70%, left bottom, from(#eee), to(#cfcfcf));
	background: -moz-linear-gradient(top,  #eee,  #cfcfcf);
	background: -o-linear-gradient(left, #eee, #cfcfcf);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#eee', endColorstr='#cfcfcf');
	background: linear-gradient(left, #eee, #cfcfcf);
}

