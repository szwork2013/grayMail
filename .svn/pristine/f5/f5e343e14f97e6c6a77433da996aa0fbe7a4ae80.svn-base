/* ================================================ Reset ================================================ */
/* html{-webkit-text-size-adjust: none;} */
html,body,p,ul,ol,li,dl,dt,dd,h1,h2,h3,h4,h5,h6,form,fieldset,legend,input,select,textarea,button,th,td,blockquote,address,var,pre{margin:0;padding:0;}
h1,h2,h3,h4,h5,h6,input,textarea,select,button,label{font-size:100%;}
button,select,textarea,input{vertical-align:middle;color:#444;}
input[type=button],input[type=submit],input[type=reset],input[type=file],button{cursor:pointer;overflow:visible;width:auto;}
button{padding:3px 5px 2px;}
textarea{resize:vertical;}  /* word-wrap for opera, resize for webkit */
a{color:#1a75ca;text-decoration:none;}
a:hover{ text-decoration:none !important;}
ul,ol{list-style:none outside none;}
img,fieldset{border:0;}
img{overflow:hidden;vertical-align:top;}
em,address,var{font-style:normal;}
sup,sub{vertical-align:baseline;}
table{border-collapse:collapse;border-spacing:0;}
i{display:inline-block;} /*用于修饰性小图标*/

/* ================================================ 字体样式 ================================================ */
body{background-color:#fff;color:#444;font:12px/1.5 "Microsoft YaHei",Verdana,"Simsun";height:100%;}
html{height:100%;}
input,select,textarea{font-family: 'Microsoft Yahei',verdana,"Simun";}
/*input IE10以上去除默认的X按钮*/
input::-ms-clear{display: none;}
::-ms-reveal{display: none;}
input{outline:none;}
mark{background:#ff0;} /* 高亮显示 */
.fz_12{font-size:12px;}
.fz_13{font-size:13px;}
.fz_14{font-size:14px;}
.fz_16{font-size:16px;}
.fz_18{font-size:18px;}
/* FontColor */
.c_000{color:#000!important;}
.gray{color:#999 !important;} /* 灰色字或单个链接 */
.black{color:#333;} /* 黑色 */
.blue{color:#1a75ca;}/*蓝色*/
.red,.errow{color:#de0202 !important;} /* 红色 */
.orange{color:#ff5907 !important;}/*橙色*/
.green{color:#008e11;}
.c_666{color:#666;}
.c_999{color:#999;}
.c_ff6600{color:#fe6600;}
.c_009900{color:#009900;}
.c_0066cc{color:#0066cc;}
.c_white, .c_white:hover{color:#FFF;}
/* FontStyle */
.fw_b{font-weight:bold!important;} /* 粗体 */
.fw_n{font-weight:normal!important;} /* 普通 */
/*font*/
.f_SimSun,.f_st{_font-family:SimSun}
/* TextAlign  */
.ta_l{text-align:left!important;}
.ta_r{text-align:right!important;}
.ta_c{text-align:center!important;}
.ta_j{text-align:justify;text-justify:inter-ideograph;}
/* ================================================ 字体样式 ================================================ */
/* Show or Hide */
.show{display:block!important;}
.hide{display:none!important;}
.v-hidden{visibility:hidden;}
.v-visible{visibility:visible;}
.pd10{padding:0 10px 0 10px;}
/*float*/
.fl{float:left;display:inline;}
.fr{float:right;display:inline;}
/* clear float */
.clearfix:after{visibility:hidden;display:block;font-size:0;content:" ";clear:both;height:0;}
.clearfix{zoom:1;}
.clearAll{overflow:hidden;_zoom:1;}

.headerCon, .down{width:950px; margin:0 auto;}

.header{background:url(../../../images/module/largeattachments/header_bg_01.png) repeat-x; height:66px;position:relative;z-index:99; min-width:inherit;}
.headerCon .logo{float:left; position:static;}
.headerCon .logoCon{float:left; border-left:1px solid #5889b5; height:26px; line-height:26px; margin:20px 0 0 15px; _display:inline; padding-left:12px; color:#fff; font-size:16px;}
.headerCon .login{float:right; font-size:14px; margin-top:20px;}
.headerCon .login li{float:left; margin-left:10px; _display:inline;}
.headerCon .login li a{color:#fff;}
.headerCon .login li span{color:#a8d6ff; border-right:1px solid #5889b5; height:14px; line-height:14px; padding-right:10px;}

.downBox{border-bottom:1px solid #e8e8e8;}
.down table{border-collapse:collapse; table-layout:fixed; width:100%;}
.down table td{padding:10px 0;}
.downCon tr td{padding:15px 0; border-bottom:1px solid #e8e8e8;}
.downCon td img{margin-left:4px;}
.fileCon{margin-left:20px;}
.fileNane{color:#444; font-size:14px;}
.fileNane span{color:#999; font-size:12px; margin-left:5px;}
.fileStatus{color:#999; line-height:24px; _margin-top:5px;}
.fileStatus i{margin:-2px 5px 0 0;}

.backgroundBG{background:#000; filter:alpha(opacity=40); -moz-opacity:0.4; -khtml-opacity:0.8; opacity:0.4; position:absolute; left:0; top:0; width:100%; height:100%;}
.loginBox{background-color:#fff; position:absolute; left:50%; top:30%; width:260px; padding:0 30px; margin-left:-160px; padding-bottom:26px;
	-moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius:3px;
}
.loginBox .close{position:absolute; right:8px; top:8px;}
.loginEntrance{margin-top:25px; font-size:14px;}
.loginEntrance .on{color:#444;font-weight:700;}

.loginInput{margin-top:18px; position:relative; zoom:1;}
.loginInput li{margin-bottom:14px;}
.loginInput .prompt{position:absolute; left:0; top:-18px; margin:0;}
.input{border:1px solid #cecece; width:258px; height:40px; line-height:40px; background-color:#f8f8f8;
	-moz-box-shadow:1px 0px 3px #eae9e9 inset;
    -webkit-box-shadow:1px 0px 3px #eae9e9 inset;
    box-shadow:1px 0px 3px #eae9e9 inset;
	-moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius:3px;
}
/*登录框*/
.input .inputCon{width:162px; height:40px; line-height:40px; color:#a8a8a8; font-size:14px; border:none; background:none; padding-left:8px; float:left;}
.input strong{float:right; margin-right:10px; _display:inline; font-size:14px; color:#666;}
.input .inputPassword{width:94%;}
.inputSmsPassword{width:160px;}
.inputSmsPassword .inputCon{width:150px;}
.loginInput .btnGreen, .loginInput .btnGreen:hover{width:260px; padding:0; text-align:center;}
.phoneNumLogin .inputPassword{width:162px;}
.phoneNumLogin .btnBlue{display:inline-block;}
