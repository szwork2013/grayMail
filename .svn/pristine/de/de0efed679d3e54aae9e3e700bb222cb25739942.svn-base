@charset "utf-8";
/*reset*/
html,body,div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, form, fieldset, legend, input, button, textarea, p, th, td{margin:0;padding:0;}
table{border-collapse:collapse;border-spacing:0;}
fieldset, img{border:0;}
caption, em, strong, th, optgroup{font-style:inherit;font-weight:inherit;}
li{list-style:none;}
caption, th{text-align:left;}
h1, h2, h3, h4, h5, h6{font-size:100%;font-weight:normal;}
legend{color:#000;}
input, button, textarea, select, optgroup, option{ font-family:inherit;font-size:inherit;font-style:inherit;font-weight:inherit;}
input, button, textarea, select{#font-size:100%;}
a{text-decoration:none;}
a:hover{text-decoration:underline;}
/*****ͨðť*****/
button{/*for ie6 ie7 ַ*/
	*padding:0 4px;
	*overflow:visible;
}
input[type = "submit"],
input[type = "reset"],
input[type = "button"],
button{
	cursor:pointer;
	padding:0 10px;
	*height:25px;
	*margin-right:4px;
	padding:2px 12px 0 12px\9;
	height:25px\9;
}
button{/*for ie6*/
	_line-height:15px;
	_height:15px;
	_padding:3px 12px 0 12px;
	_margin-right:4px
}
@-moz-document url-prefix(){/*forFF*/
	input[type = "submit"],
	input[type = "reset"],
	input[type = "button"],
	button{
		padding:1px 8px;
	}
}
@media all and (-webkit-min-device-pixel-ratio:10000), 
not all and (-webkit-min-device-pixel-ratio:0) {/*opear */
	input[type = "checkbox"] { position:inherit; }
	input[type = "submit"],
	input[type = "reset"],
	input[type = "button"],
	button{cursor:pointer;padding:4px 12px;}
}
/*for chrome and safari*/
@media screen and (-webkit-min-device-pixel-ratio:0) {
	input[type = "submit"],
	input[type = "reset"],
	input[type = "button"],
	button{padding:3px 12px 4px 12px;}
}
