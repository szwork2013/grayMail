        .formArea table{table-layout:auto;}
        .formArea th{font-weight:lighter;text-align:right;padding-right:5px;height:30px;padding-top:8px;_padding-top:5px;vertical-align:middle;font-family: "Microsoft YaHei";}
        .formArea td{vertical-align:middle;}
