<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>音乐专辑</title>
    <meta content="text/html; charset=utf-8" http-equiv="Content-Type" />

    <script type="text/javascript">
        //设域
		document.domain = window.location.host.match(/([^.]+\.[^.:]+):?\d*$/)[1].replace(/:\d+/, "");
        var diskWindow = top.$("iframe#diskDev")[0].contentWindow;
        var scripts = [{ dirName: "js/", fileName:"m2011.disk.recordproperty.js"}];
        diskWindow.CommonInit(document, diskWindow, scripts);
    </script>

    <style type="text/css">
        .h
        {
            display: none;
        }
    </style>
</head>
<body>
    <div class="dialog">
        <div class="bd">
            <table cellpadding="0" class="tbl-dialog-form dl-form-property">
                <tr class="t-edition" style="display: block">
                    <th>
                        专辑名称：
                    </th>
                    <td>
                        <input class="text" type="text" maxlength="20" />
                        <p class="dialog-error-tip" style="display: none">
                            <i class="i-warn"></i><span id="errorMsg">错误信息提示</span></p>
                    </td>
                </tr>
                <tr class="h">
                    <th>
                        类型：
                    </th>
                    <td>
                        音乐专辑
                    </td>
                </tr>
                <tr class="h">
                    <th>
                        位置：
                    </th>
                    <td id="tdPos">
                    </td>
                </tr>
                <tr class="h">
                    <th>
                        文件信息：
                    </th>
                    <td id="tdFileInfo">
                    </td>
                </tr>
                <tr class="h">
                    <th>
                        创建时间：
                    </th>
                    <td id="tdTime">
                    </td>
                </tr>
            </table>
        </div>
        <div class="ft">
            <ul class="dbtn-list">
                <li><a class="btnNormal" id="aSumbit" href="">确 定</a></li>
                <li><a class="btnNormal" id="aCancel" href="">取 消</a></li>
            </ul>
        </div>
    </div>
</body>
</html>
