	htmlEncode : function($){
	    if(typeof $=="undefined"){
	      return "";
	    }
	    $=$.replace(/&/g,"&amp;");
	    $=$.replace(/</g,"&lt;");
	    $=$.replace(/>/g,"&gt;");
	    $=$.replace(/\"/g,"&quot;");
	    $=$.replace(/ /g,"&nbsp;");
	    $=$.replace(/&amp;#([^\;]+);/ig,"&#$1;");
	    return $;
	}
};
String.prototype.format=function(){
  var str=this,tmp;
  for(var i=0;i<arguments.length;i++){
    tmp=String(arguments[i]).replace(/\$/g,"$$$$");
    str=str.replace(eval("/\\{"+i+"\\}/g"),tmp);
