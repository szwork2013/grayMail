.blackbanner {position: absolute;height: 270px;background-color: #FFF;top: 75px;width: 600px;filter: alpha(opacity=70);-moz-opacity: 0.7;-khtml-opacity: 0.7;opacity: 0.7;left:0;}
.calendarPanel {position: absolute;	z-index: 10000;	border: 1px solid #ccc;	width: 300px;	height: 310px;	background-color: #fff;	padding: 0px;-moz-box-shadow: 0px 0px 5px rgba(0,0,0,.2);-webkit-box-shadow: 0px 0px 5px rgba(0,0,0,.2);box-shadow: 0px 0px 5px rgba(0,0,0,.2);
}
.calendarPanel .calendarTable{font-size:12px;}
.calendarPanel .calendarTable tr{height:25px;}
.calendarPanel .calendarTable tr th{width:40px;height:30px;margin:0;padding:0}
.calendarPanel .calendarTitle{background-color:#e9f1fb;width:300px;text-align:center;}
.calendarPanel #goPrevYear
,.calendarPanel #goNextYear
,.calendarPanel #goPrevMonth
,.calendarPanel #goNextMonth{color:#333;font-size:13px;cursor:pointer;margin-left:10px;text-decoration:underline;}
#spanNL{color:black;}
.calendarPanel #divYearList{position:absolute;z-index:999999;width:80px;background-color:#fff;border: 1px solid #ccc;padding:5px;}
.calendarPanel #divYearList li{list-style:none; font-weight:normal; width:40px;text-align:center; float:left;cursor:pointer}
.calendarPanel #divYearList .divYearListCur{font-weight:bold}
.calendarPanel #divYearList .mouseOutLiStyle{font-weight:normal}
.calendarPanel #divYearList .mouseOverLiStyle{font-weight:bold}
.calendarPanel .mouseOutLiStyle{font-weight:normal}
.calendarPanel .mouseOverLiStyle{font-weight:bold}
.calendarPanel .spanCurYear{border:0px;	width:30px;background-color:#e9f1fb;cursor:pointer;margin-left:10px;text-decoration:underline}
.calendarPanel .spanCurYearFocus{border:1px;background-color:#e9f1fb;width:30px;margin-left:10px;}
.calendarPanel #divMonthList{position:absolute;font-weight:normal;width:50px;background-color:#fff;border: 1px solid #ccc;padding:5px;}
.calendarPanel #divMonthList li{list-style:none;width:25px;float:left;cursor:pointer;}
.calendarPanel .divMonthListCur{font-weight:bold}
.calendarPanel .spanCurMonth{border:0px;width:15px;background-color:#e9f1fb;cursor:pointer;text-decoration:underline}
.calendarPanel .spanCurMonthFocus{border:1px;background-color:#e9f1fb;width:15px;}
.calendarPanel .footerButton{width:280px;height:20px;white-space:nowrap;font-size:12px;cursor:pointer;color:#333;margin-right:15px}
.calendarPanel .histdayStyle{background-color:#CCCCCC;}
.calendarPanel .todayStyle{background-color:#319eff;margin:0;padding:0;color: #FFF;}
.calendarPanel .currStyle{background-color:#fcc;margin:0;padding:0}
.calendarPanel .mouseOverStyle{background-color:#a2d0f8;text-align:center;color:#FFF;cursor:pointer;margin:0;padding:0}
.calendarPanel .mouseOutStyle{background-color:#fff;padding:0px;margin:0px;}
.calendarPanel .selectedDayStyle{background-color:#e8e8e8;padding:0px;margin:0px;}
.calendarPanel .blankStyle{background-color:#fff;font-size:10px;padding:0px;margin:0px;}
.calendarPanel .week{height:20px;}
.calendarPanel .week th{height:20px;padding:0px;margin:0px; background:#fff; border-bottom:1px solid #ccc}
.calendarPanel .calendarTable td div{ color:#757575}
.calendarPanel  .calendarTable .todayStyle div{color: #FFF;}
.calendarPanel .calendarTable td b{ caption-side:#333; font-size:14px}



