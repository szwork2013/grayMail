.writeOk_new .writeOk_ico{float:left; width:46px;}
.writeOk_info{float:left; width:485px;}
.writeOk_info h1{font-size:32px; line-height:32px; *line-height:38px; font-weight:normal;}
.writeOk_info h1 span{color:#999; font-size:12px; margin-left:5px;}
.writeOk_info_btn{margin-top:2px;}
.writeOk_info_btn a{color:#666; margin-right:21px; font-size:14px;}
.writeOk_info_btn a.restBtn{margin-right:2px;}
.writeOk_info_btn a i{margin-right:6px;}
.writeOk_info_btn a:hover{color:#444;}
.writeOk_info_pic{background:url(../../images/global/write-btn.png) no-repeat; width:485px; height:40px; margin-top:28px;}
.writeOk_info_pic span{color:#ff5907; font-size:14px; margin:10px 0 0 85px; display:inline-block; float:left;}
.writeOk_info_pic a{float:right; margin-top:8px;}
.writeOk_info_pic .i_writeBtnA{margin-right:11px; _display:inline;}
.writeOk_info_pic .i_writeBtnB{margin-right:72px; _display:inline;}
.writeOk_box{border:1px solid #e8e8e8; background-color:#fafafa; padding:0 15px 10px; margin-top:17px; position:relative; zoom:1;}
.writeOk_box_title{height:32px; line-height:32px; border-bottom:1px solid #e8e8e8; display:block; padding-left:4px;}
.writeOk_box_title .fr{float:right; font-weight:normal;}
.writeOk_box_title span{font-weight:normal; color:#666;}
.writeOk_box_list{position:relative; overflow-y:auto; margin-top:10px;}
.writeOk_box_list li{height:30px; line-height:30px; padding:0 7px;}
.writeOk_box_list .writeOk_box_input{width:82px; padding:2px 4px; border:1px solid #bbb; background-color:#fff; margin-left:6px; margin-top:-3px; *margin-top:0px; _margin-top:2px;}
.writeOk_box_list .writeOk_box_inputFocus{border:1px solid #9bbfdc; width:160px;}
.writeOk_box_list .checkbox, .writeOk_vip_list .checkbox{position:relative; top:-2px; *top:-1px;}
.writeOk_box_btn{border-top:1px solid #e8e8e8; padding-top:10px; margin-top:10px; height:28px;}
.writeOk_box_btn .btnSetG{float:left;}
.writeOk_box_btnNot{margin-top:0;}
.writeOk_box_a{height:28px; line-height:28px; display:inline-block; margin-left:16px; _display:inline;}

.writeOk_boxOther{padding-top:11px;}
.writeOk_boxOther .i_ok_min, .writeOk_boxOther .i_warn_min{float:left; width:22px; margin-left:4px;}
.writeOk_boxOther_right{float:left; width:427px;}
.writeOk_boxOther_right strong{margin-top:-1px\9; _margin-top:1px; display:block;}
:root .writeOk_boxOther_right strong{margin-top:0;}
.writeOk_boxOther_right p{color:#666; line-height:20px;}
.writeOk_boxOther_bottom{border-top:1px solid #e8e8e8; margin-top:10px; padding:10px 0 0 26px;}
.writeOk_boxOther_bottom span{color:#008e11;}

.writeOk_vip{padding:13px 0 0 2px;}
.writeOk_vip img{float:left; width:70px;}
.writeOk_vip_right{float:left; width:369px; margin-left:12px;}
.writeOk_vip_right strong{font-size:14px;}
.writeOk_vip_list{color:#666; margin-top:10px; overflow-y:auto;}
.writeOk_vip_list li{padding:2px 0;}
.writeOk_vip_list span{color:#008e11;}
.writeOk_vip_btn{height:28px; margin-top:10px;}
.writeOk_hover{background-color:#f0f0f0;}

.writeOk_box_ul{margin-top:12px;}
.writeOk_label{float:left; width:48px; text-align:right; display:inline-block; height:25px; line-height:25px;}
.writeOk_element{float:left; width:405px;}
.writeOk_box_ul_one .writeOk_label{width:72px;}
.writeOk_box_ul_one .writeOk_element{width:381px;}
.writeOk_element .writeOk_element_p{line-height:30px; color:#666;}
.writeOk_element .c_666{margin-left:7px; height:25px; line-height:25px; display:inline-block;}
.writeOk_element_btn{height:28px;}
.writeOk_element_btn .btnSetG{float:left;}
.writeOk_element_btn .gray, .writeOk_element_btn .btnSetG_right{line-height:28px; margin-left:10px; display:inline-block;}

.writeOk_boxWarp{margin:14px 0 0 -9px;}
.writeOk_boxWarp li{float:left; width:145px; text-align:center; margin-left:9px; _display:inline;}
.writeOk_boxWarp p{height:30px; line-height:30px;}
.writeOk_box_addrBas{margin-top:4px; overflow-y:auto;}
.writeOk_box_addrBasNot{height:154px;}
.writeOk_box_addrBas .addrBaseNew{margin:10px 0 0 10px;}
.writeOk_box_addrBas .addrBase_con{width:68px; overflow:hidden; white-space:nowrap; text-overflow:ellipsis; cursor:default;}
.writeOk_box_addrBas .btnNormal_writeNo .addrBase_con{width:84px;}

.writeOk_window{border:1px solid #bbb; position:absolute; left:0; top:0; width:181px; padding:5px; background-color:#fff;
	-moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius:5px;

	-moz-box-shadow:3px 3px 3px #bbb;
    -webkit-box-shadow:3px 3px 3px #bbb;
    box-shadow:3px 3px 3px #bbb;
}
.writeOk_window .writeOk_windowTop{position:absolute; left:14px; top:-12px;}
.writeOk_window_con{border:1px solid #eee; padding:8px; color:#666;
	-moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius:5px;
}
.writeOk_window_con li{line-height:22px;}

.writeOk_table{border-collapse:collapse; table-layout:fixed; width:800px; color:#666; position:relative; zoom:1; background-color:#fff; z-index:1; margin-top:5px;}
.writeOk_table th, .writeOk_table td{border:1px solid #ccc; height:25px; padding-left:7px;}
.writeOk_table th{text-align:left;}
.writeOk_table .writeOk_tableTop{position:absolute; left:190px; top:-5px; border:none; width:9px; height:6px; padding:0;}
.writeOk_box .tipsOther .tipsBottom{_bottom:-5px;}

.prelativeZindex .lineMlr{padding:0 2px;}
.prelativeZindex .button_triangle{margin-left:0;}

/*发起会议*/
.tabTitle_title{background-color:#f8f8f8; height:25px; line-height:25px; padding-left:11px; display:block;}
.meetingCon_c{position:relative; zoom:1;}
.meetingCon .writeMainDiv{margin-top:10px;}
.meetingCon .contactSide .tab{margin-top:0;}


/*写信页新input模板框*/
.colorGray{color:#c5c5c5;}
.writeTable_li{margin-bottom:10px; vertical-align:top;}
.divInputBox{border:1px solid #c5c5c5; height:26px; padding:0 8px; position:relative; zoom:1;}
.divInputBox .addrBaseNew{margin-top:4px;}
.divInputBox.divInputBox_list{height:auto; padding-bottom:2px; padding-right:30px; word-wrap:break-word; word-break:break-all;}
.divInput{border:1px solid #fff; padding:4px 0 3px; float:left; width:100%; display:block;}
.divInputBoxPor_r .divInput{width:98%;}
.divInputBoxPor_r .i-addPeople{position:absolute; right:8px; top:7px;}
.divInputBoxPor_l{padding-left:31px;}
.divInputBoxPor_l .divInput{width:99%;}
.divInputBoxPor_l .i-address{position:absolute; left:8px; top:5px;}
.accessoriesBox{margin-top:10px;}
.accessoriesBox .writeattrlist, .accessoriesBox .writeattrlist .attrlistUl li{border:none;}
.noticeBox_top .checkbox{float:left; margin-top:5px; *margin-top:1px; _margin-top:0;}
.noticeBox_top label{float:left; margin:2px 0 0 5px;}
.noticeBox_top .iText{float:left; width:465px; margin-left:8px;}
.noticeBox_bottom{margin-left:76px; *margin-left:81px; line-height:22px; margin-top:2px;}
.writeTable_text{height:auto;}
.writeTable_text textarea{width:100%; border:1px solid #fff; resize:none; overflow:auto; padding:5px 0; height:240px;}

/*全局底部按钮*/
.allFrameBtn_bottom{border-top:1px solid #e8e8e8; padding:10px 0 9px;}
.allFrameBtn_bottom .btnG, .allFrameBtn_bottom .btnG span{height:28px; line-height:28px;}
.allFrameBtn_bottom .btnTb, .allFrameBtn_bottom .btnTb span{height:26px; line-height:26px;}
.allFrameBtn_bottom .btnG{padding-left:20px;}
.allFrameBtn_bottom	.btnG span{padding-right:20px;}

/*写信页联系人搜索框*/
.searchContactText{width:141px;height:12px;line-height:12px;padding:5px;border:1px solid #c5c5c5;border-bottom-color:#c6c6c6;border-bottom-color:#d9d9d9;color:#666;float:left;border-right:none;}
.searchContactBtn{cursor:pointer;width:22px;height:22px;background:white;overflow:hidden;float:left;border:1px solid #c5c5c5;border-bottom-color:#c6c6c6;border-bottom-color:#d9d9d9;border-right-color:#d9d9d9;border-left:none;}
